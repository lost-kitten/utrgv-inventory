<script>
$(document).ready(function() {
      $('#campus').bind('change', function(e) {
         if ($('#campus').val() == 'BROWNSVILLE') {
            $('#bbuilding').show();
            $("#bbuilding").css({
               display: "inline-block"
            });
            $('#ebuilding, #mbuilding, #hbuilding, #rbuilding').hide();
         } else if ($('#campus').val() == 'EDINBURG') {
         $('#ebuilding').show();
          $("#ebuilding").css({
               display: "inline-block"
            });
            $('#bbuilding, #mbuilding, #hbuilding, #rbuilding').hide();

