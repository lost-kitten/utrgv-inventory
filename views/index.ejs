         $('#mbuilding').show();
          $("#mbuilding").css({
               display: "inline-block"
            });
            $('#bbuilding, #ebuilding, #hbuilding, #rbuilding').hide();

         }  else if ($('#campus').val() == 'RGC') {
         $('#rbuilding').show();
          $("#rbuilding").css({
               display: "inline-block"
            });
            $('#bbuilding, #mbuilding, #hbuilding, #ebuilding').hide();

         }
      }).trigger('change');
   });
</script>
